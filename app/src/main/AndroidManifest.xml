<?xml version="1.0" encoding="utf-8"?>
<manifest package="ch.riverworld.collector"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/collector"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".InfoActivity"/>
        <activity android:name=".FilterResult"/>
        <activity android:name=".DetailsActivity"/>
        <activity android:name=".LentActivity"/>
        <activity android:name=".FriendsActivity"/>
        <activity android:name=".DatabaseActivity"/>
        <activity android:name=".ItemActivity"/>
        <activity android:name=".CollectionActivity">
        </activity>
        <activity android:name=".TableActivity">
        </activity>
    </application>

</manifest>
